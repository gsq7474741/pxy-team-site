        "Supervisor",
        "Ph.D. Student",
        "Master Student",
        "Alumni"
      ]
    },
    "enrollment_year": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "bio": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "enrollment_status": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "enum": [
        "Current",
        "Graduated"
      ]
    },
    "research_interests": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "education_background": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
