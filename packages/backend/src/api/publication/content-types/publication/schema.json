{
  "kind": "collectionType",
  "collectionName": "publications",
  "info": {
    "singularName": "publication",
    "pluralName": "publications",
    "displayName": "Publication"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "authors": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "publication_venue": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "year": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "volume_issue_pages": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "publication_type": {
      "type": "enumeration",
      "enum": ["Journal", "Conference"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "doi_link": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "pdf_file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["files"],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "code_link": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "abstract": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "research_areas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::research-area.research-area",
      "inversedBy": "related_publications"
    }
  }
}
